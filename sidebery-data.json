  "sidebarCSS": "#root.root {--tabs-activated-fg: #65e575ff;}\n#root.root {--tabs-discarded-title-opacity: 0.01;}\n#root.root {--tabs-normal-fg: #dad7abff;}\n#root.root {--tabs-color-layer-opacity: 0.5;}\n#root.root {--tabs-activated-color-layer-opacity: 0.63;}\n\n#root.root {--tabs-discarded-favicon-opacity: 0.28;}\n\n\n\n\n\n#root.root {--status-err: #df6464;}\n#root.root {--windows-popup-window-border: rgb(255, 205, 108);}\n#root.root {--tabs-activated-bg: #346aafff;}\n\n#root.root {--tabs-lvl-opacity: 0.6;}\n\n#root:not(:hover) .TabsPanel {--tabs-indent: 0px;}\n\n\n\n.Tab {\n  border: 1px solid transparent; /* Default border */\n  border-radius: 5px; /* Rounded corners */\n  transition: all 0.3s ease;\n}\n\n.Tab:hover {\n  border-color: #61dafb; /* Highlight border on hover */\n}\n\n.Tab.active {\n  border-color: #61dafb; /* Active tab border color */\n}\n\n#root {\n  --tabs-font-size: 14px; /* Font size for tabs */\n  --tabs-line-height: 1.5; /* Line height for better readability */\n  --tabs-padding: 10px; /* Padding for tabs */\n}\n\n\n\n.Bookmark {\n  background-color: #3c4043; /* Background for bookmarks */\n  border-radius: 4px; /* Rounded corners for bookmarks */\n}\n\n.Bookmark.folder {\n  font-weight: bold; /* Bold text for folders */\n  color: #f39c12; /* Distinct color for folders */\n}",
  "ver": "5.3.1"
